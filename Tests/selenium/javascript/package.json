{
  "name": "selenium_js_cucumber",
  "version": "1.0.0",
  "description": "Automation framework with Selenium, Javascript and Cucumber",
  "main": "index.js",
  "scripts": {
    "test:local": "set CURRENT_ENV=local&& cucumber-js --config cucumber.json",
    "test:dev": "export CURRENT_ENV=dev&& cucumber-js --config cucumber.json",
    "test": "export CURRENT_ENV=test&& cucumber-js --config cucumber.json",
    "debug": "set CURRENT_ENV=local&& node --inspect-brk ./node_modules/@cucumber/cucumber/bin/cucumber-js --config cucumber.json"
  },
  "author": "Srivatsan S",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^9.0.0",
    "@faker-js/faker": "^7.6.0",
    "axios": "^1.3.4",
    "chai": "^4.3.7",
    "mongodb": "^5.1.0",
    "selenium-webdriver": "^4.8.1"
  }
}
